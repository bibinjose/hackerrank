   "source": [
    "from math import sqrt\n",
    "#input the size of the array\n",
    "size = int(input())\n",
    "\n",
    "#Enter the array X of numbers\n",
    "numbers = list(map(int, input().split()))\n",
    "mean = sum(numbers)/size\n",
    "SD=sqrt(sum([(x-mean)**2 for x in numbers])/size)\n",
    "print(round(SD,1))"
   ]
